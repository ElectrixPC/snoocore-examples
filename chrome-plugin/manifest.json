{
  "manifest_version": 2,
  "name": "TopAww",
  "description": "Shows the top post from /r/aww",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://oauth.reddit.com/",
    "https://www.reddit.com/"
  ],
  "background": {
    "scripts": [ "Snoocore-browser.min.js", "background.js" ],
    "persistent": false
  }
}
